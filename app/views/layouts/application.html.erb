<!DOCTYPE html>
<html>
  <head>
    <title>MoveTime</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="search-form">
      <%= form_tag :search, method: :get, id: "searching" do %>
        <form>
          <%= text_field_tag :city_state, nil, placeholder: "City, State" %>
          <%= submit_tag "Search" %>
        </form>
      <% end %>
    </div>
    <%= yield %>
  </body>
</html>
